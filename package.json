{
   "name": "dashboar_backend",
   "version": "0.0.1",
   "description": "<p align=\"center\">\r   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"200\" alt=\"Nest Logo\" /></a>\r </p>",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "husky": {
      "hooks": {
         "pre-commit": "pretty-quick --staged"
      }
   },
   "scripts": {
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:create": "npm run typeorm migration:create src/migrations/auto",
      "migration:generate ": "npm run typeorm migration:generate --  -d src/typeorm.config.ts  src/migrations/auto",
      "migration:revert": " npm run typeorm migration:revert -- -d src/typeorm.config.ts",
      "migration:run": "npm run typeorm migration:run -- -d src/typeorm.config.ts",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json"
   },
   "dependencies": {
      "@nestjs/common": "^10.0.0",
      "@nestjs/config": "^3.2.2",
      "@nestjs/core": "^10.0.0",
      "@nestjs/mapped-types": "^2.0.5",
      "@nestjs/platform-express": "^10.0.0",
      "@nestjs/swagger": "^7.3.1",
      "@nestjs/typeorm": "^10.0.2",
      "@next/env": "^14.2.3",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "dotenv": "^16.4.5",
      "dotenv-flow": "^4.1.0",
      "husky": "^9.0.11",
      "mongoose": "^6.2.2",
      "pg": "^8.4.0",
      "pre-commit": "^1.2.2",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1",
      "swagger-ui-express": "^5.0.0",
      "typeorm": "0.3.20"
   },
   "devDependencies": {
      "@eslint/compat": "^1.0.3",
      "@eslint/js": "^9.4.0",
      "@nestjs/cli": "^10.3.2",
      "@nestjs/schematics": "^10.0.1",
      "@nestjs/testing": "^10.0.0",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.2",
      "@types/node": "^16.11.10",
      "@types/supertest": "^6.0.0",
      "@typescript-eslint/eslint-plugin": "^6.0.0",
      "@typescript-eslint/parser": "^6.0.0",
      "eslint": "^8.57.0",
      "globals": "^15.4.0",
      "jest": "^29.5.0",
      "prettier": "^3.3.1",
      "pretty-quick": "^4.0.0",
      "source-map-support": "^0.5.21",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.0",
      "ts-loader": "^9.4.3",
      "ts-node": "10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "5.4.5",
      "typescript-eslint": "^7.13.0"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   },
   "main": ".eslintrc.js",
   "directories": {
      "test": "test"
   }
}
